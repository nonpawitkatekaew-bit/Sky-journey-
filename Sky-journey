    <!-- QR Code Modal (Hidden by default) -->
    <div id="qr-modal">
        <h3 class="text-2xl font-bold mb-2 text-white">‡πÅ‡∏ä‡∏£‡πå‡πÉ‡∏´‡πâ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡πÄ‡∏•‡πà‡∏ô</h3>
        <p class="text-blue-300 mb-6">‡∏™‡πÅ‡∏Å‡∏ô QR Code ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡∏°</p>
        <div id="qrcode"></div>
        <button id="close-qr" class="bg-white/10 text-white px-8 py-3 rounded-full border border-white/20 font-bold">‡∏Å‡∏•‡∏±‡∏ö‡∏´‡∏ô‡πâ‡∏≤‡∏´‡∏•‡∏±‡∏Å</button>
    </div>

    <!-- Start Screen -->
    <div id="start-screen" class="overlay-screen">
        <div class="astronaut-svg">
            <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M12 2C8.13 2 5 5.13 5 9V11C5 11.55 5.45 12 6 12H18C18.55 12 19 11.55 19 11V9C19 5.13 15.87 2 12 2Z" fill="#E2E8F0"/>
                <path d="M16 8H8V10H16V8Z" fill="#1E293B"/>
                <path d="M7 13V18C7 19.1 7.9 20 9 20H15C16.1 20 17 19.1 17 18V13H7Z" fill="#CBD5E1"/>
                <rect x="9" y="14" width="6" height="4" rx="1" fill="#94A3B8"/>
                <circle cx="12" cy="5" r="1" fill="#94A3B8"/>
