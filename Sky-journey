            </svg>
        </div>
        <h1 class="text-6xl font-bold mb-2 tracking-tight">Sky journey</h1>
        <p class="text-blue-300 mb-10 text-xl opacity-90">‡∏°‡∏≤‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏ä‡∏±‡πâ‡∏ô‡∏ö‡∏£‡∏£‡∏¢‡∏≤‡∏Å‡∏≤‡∏®‡πÇ‡∏•‡∏Å‡∏Å‡∏±‡∏ô‡πÄ‡∏ñ‡∏≠‡∏∞</p>
        
        <button id="start-btn" class="btn-start">‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏£‡∏ß‡∏à</button>
        <!-- ‡∏õ‡∏∏‡πà‡∏° QR Code ‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡πÄ‡∏´‡πá‡∏ô‡∏ä‡∏±‡∏î‡πÄ‡∏à‡∏ô -->
        <button id="qr-trigger" class="btn-qr">üì± ‡∏™‡∏£‡πâ‡∏≤‡∏á QR Code ‡πÉ‡∏´‡πâ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô</button>
    </div>

    <!-- End Screen -->
    <div id="end-screen" class="overlay-screen" style="display: none;">
        <div class="text-9xl mb-8 text-yellow-400">üèÜ</div>
        <h2 class="text-5xl font-bold text-yellow-400 mb-6">‡∏†‡∏≤‡∏£‡∏Å‡∏¥‡∏à‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!</h2>
        <p class="text-blue-100 mb-14 px-12 text-xl leading-relaxed">
            ‡∏Ñ‡∏∏‡∏ì‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏≤‡∏á‡∏ñ‡∏∂‡∏á 1,000 ‡∏Å‡∏°. ‡πÅ‡∏•‡∏∞‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏™‡∏ñ‡∏≤‡∏ô‡∏µ‡∏≠‡∏ß‡∏Å‡∏≤‡∏®‡∏ô‡∏≤‡∏ô‡∏≤‡∏ä‡∏≤‡∏ï‡∏¥ (ISS) ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß!
        </p>
        <button onclick="location.reload()" class="bg-blue-600 px-14 py-5 rounded-full text-white font-bold text-2xl shadow-xl">‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á</button>
    </div>

    <!-- Game Controls (Mobile) -->
    <div class="controls" id="game-controls" style="display: none;">
        <div id="l-btn" class="ctrl-btn">‚óÄ</div>
        <div id="r-btn" class="ctrl-btn">‚ñ∂</div>
    </div>

    <canvas id="canvas"></canvas>
</div>

<script>
    const canvas = document.getElementById('canvas');
    const ctx = canvas.getContext('2d');
    const altVal = document.getElementById('alt-val');
    const bottomInfo = document.getElementById('bottom-info');
    const infoTitle = document.getElementById('info-title');
    const infoText = document.getElementById('info-text');
    const infoIcon = document.getElementById('info-icon');
    
    let gameActive = false;
    let cameraY = 0;
    let player, platforms = [], rain = [], satellites = [], meteors = [], planes = [], auroraOffset = 0;
    let moveLeft = false, moveRight = false;
    let currentIdx = -1;
    let audioCtx, masterGain, musicInited = false;
    let hasReachedGoal = false;

    // ‡∏£‡∏∞‡∏ö‡∏ö QR Code Logic
    const qrTrigger = document.getElementById('qr-trigger');
    const qrModal = document.getElementById('qr-modal');
    const closeQr = document.getElementById('close-qr');
    const qrContainer = document.getElementById('qrcode');

    qrTrigger.onclick = () => {
        qrContainer.innerHTML = ''; // ‡∏•‡πâ‡∏≤‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏Å‡πà‡∏≤
        // ‡∏™‡∏£‡πâ‡∏≤‡∏á QR ‡∏à‡∏≤‡∏Å URL ‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
        new QRCode(qrContainer, {
            text: window.location.href,
            width: 220,
            height: 220,
            colorDark : "#000000",
            colorLight : "#ffffff",
            correctLevel : QRCode.CorrectLevel.H
        });
        qrModal.style.display = 'flex';
    };

    closeQr.onclick = () => {
        qrModal.style.display = 'none';
    };

    const layers = [
        { th: '‡πÇ‡∏ó‡∏£‡πÇ‡∏û‡∏™‡πÄ‡∏ü‡∏µ‡∏¢‡∏£‡πå', start: 0, end: 12, color: '#87CEEB', icon: 'üåßÔ∏è', info: '‡∏ä‡∏±‡πâ‡∏ô‡∏•‡πà‡∏≤‡∏á‡∏™‡∏∏‡∏î‡∏ó‡∏µ‡πà‡∏°‡∏µ‡πÄ‡∏°‡∏Ü‡∏´‡∏ô‡∏≤‡πÅ‡∏•‡∏∞‡∏ù‡∏ô‡∏ï‡∏Å' },
        { th: '‡∏™‡∏ï‡∏£‡∏≤‡πÇ‡∏ó‡∏™‡πÄ‡∏ü‡∏µ‡∏¢‡∏£‡πå', start: 12, end: 50, color: '#1E90FF', icon: '‚úàÔ∏è', info: '‡∏ä‡∏±‡πâ‡∏ô‡πÇ‡∏≠‡πÇ‡∏ã‡∏ô ‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏ô‡∏¥‡πà‡∏á ‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡πÅ‡∏Å‡πà‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô' },
        { th: '‡πÄ‡∏°‡πÇ‡∏ã‡∏™‡πÄ‡∏ü‡∏µ‡∏¢‡∏£‡πå', start: 50, end: 85, color: '#00008B', icon: '‚òÑÔ∏è', info: '‡∏ä‡∏±‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏¢‡πá‡∏ô‡∏à‡∏±‡∏î ‡∏ä‡πà‡∏ß‡∏¢‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏≠‡∏∏‡∏Å‡∏Å‡∏≤‡∏ö‡∏≤‡∏ï' },
        { th: '‡πÄ‡∏ó‡∏≠‡∏£‡πå‡πÇ‡∏°‡∏™‡πÄ‡∏ü‡∏µ‡∏¢‡∏£‡πå', start: 85, end: 600, color: '#191970', icon: '‚ú®', info: '‡∏ä‡∏±‡πâ‡∏ô‡∏™‡∏∞‡∏ó‡πâ‡∏≠‡∏ô‡∏Ñ‡∏•‡∏∑‡πà‡∏ô‡∏ß‡∏¥‡∏ó‡∏¢‡∏∏‡πÅ‡∏•‡∏∞‡πÄ‡∏Å‡∏¥‡∏î‡πÅ‡∏™‡∏á‡∏≠‡∏≠‡πÇ‡∏£‡∏£‡πà‡∏≤' },
        { th: '‡πÄ‡∏≠‡πá‡∏Å‡πÇ‡∏ã‡∏™‡πÄ‡∏ü‡∏µ‡∏¢‡∏£‡πå', start: 600, end: 1000, color: '#020617', icon: 'üõ∞Ô∏è', info: '‡∏ä‡∏±‡πâ‡∏ô‡∏ô‡∏≠‡∏Å‡∏™‡∏∏‡∏î ‡∏ß‡∏á‡πÇ‡∏Ñ‡∏à‡∏£‡∏Ç‡∏≠‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏µ‡∏≠‡∏ß‡∏Å‡∏≤‡∏®' }
    ];

    const PIXELS_PER_LAYER = 3500; 
    const GOAL_Y = -(PIXELS_PER_LAYER * layers.length);

    // --- Audio System ---
    function initMusic() {
        if (musicInited) return;
        audioCtx = new (window.AudioContext || window.webkitAudioContext)();
        masterGain = audioCtx.createGain();
        masterGain.gain.setValueAtTime(0.1, audioCtx.currentTime);
        masterGain.connect(audioCtx.destination);

        const melody = [440, 493.88, 523.25, 659.25];
        let noteIdx = 0;
        setInterval(() => {
            if (!gameActive) return;
            const osc = audioCtx.createOscillator();
            const g = audioCtx.createGain();
            osc.type = 'triangle';
            osc.frequency.setValueAtTime(melody[noteIdx % melody.length], audioCtx.currentTime);
            g.gain.setValueAtTime(0.05, audioCtx.currentTime);
            g.gain.exponentialRampToValueAtTime(0.001, audioCtx.currentTime + 1.2);
            osc.connect(g); g.connect(masterGain);
            osc.start(); osc.stop(audioCtx.currentTime + 1.2);
            noteIdx++;
        }, 1200);
        musicInited = true;
    }

    // --- Gameplay Core ---
    class Player {
        constructor() {
            this.x = canvas.width / 2;
            this.y = canvas.height - 200;
            this.vx = 0; this.vy = 0;
            this.gravity = 0.58; this.jump = -18;
            this.w = 36; this.h = 45;
        }
        draw() {
            ctx.save();
            ctx.translate(this.x, this.y - cameraY);
            ctx.fillStyle = 'white';
            ctx.beginPath(); ctx.roundRect(-this.w/2, -this.h, this.w, this.h, 8); ctx.fill();
            ctx.fillStyle = '#38bdf8';
            ctx.beginPath(); ctx.roundRect(-14, -40, 28, 18, 5); ctx.fill();
            ctx.restore();
        }
        update() {
            if (moveLeft) this.vx -= 1.6;
            if (moveRight) this.vx += 1.6;
            this.vx *= 0.85; this.x += this.vx;
            this.vy += this.gravity; this.y += this.vy;
            if (this.x < 0) this.x = canvas.width;
            if (this.x > canvas.width) this.x = 0;
            if (this.y < cameraY + canvas.height * 0.45) cameraY = this.y - canvas.height * 0.45;
        }
    }

    class Platform {
        constructor(y, type = 'normal') {
            this.y = y; this.type = type;
            this.w = (type === 'start' || type === 'goal') ? canvas.width : 95; 
            this.h = (type === 'start' || type === 'goal') ? 120 : 18;
            this.x = (type === 'start' || type === 'goal') ? 0 : Math.random() * (canvas.width - this.w);
        }
        draw() {
            let relY = this.y - cameraY;
            if (this.type === 'ghost') ctx.globalAlpha = 0.4;
            if (this.type === 'start') {
                ctx.fillStyle = '#334155'; ctx.fillRect(0, relY, canvas.width, 300);
            } else if (this.type === 'goal') {
                ctx.fillStyle = '#1e293b'; ctx.fillRect(0, relY, canvas.width, 800);
                ctx.fillStyle = '#fbbf24'; ctx.fillRect(0, relY, canvas.width, 35);
                ctx.fillStyle = 'white'; ctx.font = 'bold 24px Kanit'; ctx.textAlign = 'center';
                ctx.fillText('üõ∞Ô∏è ISS - ‡∏™‡∏ñ‡∏≤‡∏ô‡∏µ‡∏≠‡∏ß‡∏Å‡∏≤‡∏®‡∏ô‡∏≤‡∏ô‡∏≤‡∏ä‡∏≤‡∏ï‡∏¥', canvas.width/2, relY + 130);
            } else {
                ctx.fillStyle = this.type === 'gold' ? '#fbbf24' : 'white';
                ctx.beginPath(); ctx.roundRect(this.x, relY, this.w, this.h, 10); ctx.fill();
            }
            ctx.globalAlpha = 1.0;
        }
    }

    function initGame() {
        player = new Player();
        platforms = []; rain = []; satellites = []; meteors = []; planes = [];
        cameraY = 0; currentIdx = -1; hasReachedGoal = false;
        platforms.push(new Platform(canvas.height - 120, 'start'));
        for (let i = 1; i < 20; i++) {
            platforms.push(new Platform(canvas.height - 120 - i * 145));
        }
    }

    function animate() {
        if (!gameActive) return;

        let absY = Math.abs(cameraY);
        let layerIdx = Math.max(0, Math.min(Math.floor(absY / PIXELS_PER_LAYER), layers.length - 1));

        if (layerIdx !== currentIdx) {
            currentIdx = layerIdx;
            const l = layers[currentIdx];
            infoIcon.innerText = l.icon; infoTitle.innerText = l.th; infoText.innerText = l.info;
            bottomInfo.classList.add('active');
            setTimeout(() => bottomInfo.classList.remove('active'), 6000);
        }

        ctx.fillStyle = layers[currentIdx].color;
        ctx.fillRect(0, 0, canvas.width, canvas.height);

        player.update();
        let altitude = Math.min(1000, Math.floor(Math.abs(player.y - (canvas.height-200)) / 17.5));
        altVal.innerText = altitude;

        // Collision logic
        if (player.vy > 0) {
            for (let p of platforms) {
                if (p.type === 'ghost') continue;
                if (player.x + 20 > p.x && player.x - 20 < p.x + p.w && 
                    player.y > p.y && player.y < p.y + p.h + 20) {
                    if (p.type === 'goal') {
                        player.vy = 0; player.y = p.y; hasReachedGoal = true;
                        gameActive = false;
                        setTimeout(() => document.getElementById('end-screen').style.display = 'flex', 800);
                        break;
                    }
                    player.vy = p.type === 'gold' ? player.jump * 2.2 : player.jump;
                    player.y = p.y; break;
                }
            }
        }

        // Infinite generation
        const lastP = platforms[platforms.length - 1];
        if (lastP.y > cameraY - 500) {
            if (lastP.y <= GOAL_Y + 300) {
                if (!platforms.some(x => x.type === 'goal')) platforms.push(new Platform(GOAL_Y, 'goal'));
            } else {
                platforms.push(new Platform(lastP.y - 145, Math.random() > 0.9 ? 'gold' : 'normal'));
            }
        }

        platforms.forEach(p => p.draw());
        player.draw();

        if (player.y - cameraY > canvas.height + 300) initGame();
        requestAnimationFrame(animate);
    }

    document.getElementById('start-btn').onclick = () => {
        initMusic();
        document.getElementById('start-screen').style.display = 'none';
        document.getElementById('game-hud').style.display = 'flex';
        document.getElementById('audio-btn').style.display = 'flex';
        document.getElementById('game-controls').style.display = 'flex';
        initGame(); gameActive = true; animate();
    };

    // Controls
    const lBtn = document.getElementById('l-btn'), rBtn = document.getElementById('r-btn');
    lBtn.ontouchstart = (e) => { e.preventDefault(); moveLeft = true; };
    lBtn.ontouchend = () => moveLeft = false;
    rBtn.ontouchstart = (e) => { e.preventDefault(); moveRight = true; };
    rBtn.ontouchend = () => moveRight = false;
    
    window.onkeydown = (e) => { if(e.key === 'ArrowLeft') moveLeft = true; if(e.key === 'ArrowRight') moveRight = true; };
    window.onkeyup = (e) => { if(e.key === 'ArrowLeft') moveLeft = false; if(e.key === 'ArrowRight') moveRight = false; };

    function resize() { canvas.width = canvas.parentElement.clientWidth; canvas.height = canvas.parentElement.clientHeight; }
    window.onresize = resize; resize();
</script>
</body>
</html>

